            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="ancCardId" alignment="CENTER" layoutX="251.0" layoutY="47.0" prefHeight="27.0" prefWidth="274.0" style="-fx-border-color: #000000;">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <RadioButton fx:id="ancDebit" layoutX="362.0" layoutY="81.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="52.0" text="DEBIT">
         <toggleGroup>
            <ToggleGroup fx:id="GP2" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="ancCredit" layoutX="425.0" layoutY="81.0" mnemonicParsing="false" text="CREDIT" toggleGroup="$GP2" />
      <Label layoutX="265.0" layoutY="81.0" text="TYPE OF CARD">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="ancAmount" layoutX="362.0" layoutY="103.0" />
      <Label layoutX="293.0" layoutY="107.0" text="AMOUNT">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="319.0" layoutY="137.0" />
      <Label layoutX="306.0" layoutY="137.0" text="BADGE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="ancBadge" layoutX="362.0" layoutY="133.0" />
      <Button layoutX="322.0" layoutY="173.0" mnemonicParsing="false" onAction="#addNewCard" prefHeight="25.0" prefWidth="133.0" text="CREATE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</Pane>
